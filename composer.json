{
    "name": "jamarcer/symfony-console-apm",
    "description": "Elastic APM for Symfony Console",
    "version": "2.0.0",
    "license": "MIT",
    "type": "library",
    "authors": [
    {
        "name": "Jesús Martínez",
        "email": "jamarcer@gmail.com"
    }
    ],
    "autoload": {
        "psr-4": {
            "Jamarcer\\DDDDoctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Jamarcer\\Tests\\DDDDoctrine\\": "tests/"
        }
    },
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": "^8.0",
        "symfony/console": "^5.0",
        "jamarcer/ddd-framework": "dev-main",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "dg/bypass-finals": "^1.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jamarcer/ddd-framework"
        }
    ]
}
